@model List<Picture>

<section class="awe-section-7">
    <section class="section_brand">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="content_module">
                        <div class="block-brand owl-carousel"
                             data-dot="true"
                             data-nav='false'
                             data-lg-items='6'
                             data-md-items='4'
                             data-sm-items='4'
                             data-xs-items='2'
                             data-margin='0'>

                             @foreach (var item in Model)
                            {
                                <a href="#" class="">
                                    <span class="wrp effect-2">
                                        <img src="@item.Avatar" data-lazyload="@item.Avatar"
                                              title="demo">
                                    </span>
                                </a>
                            }
                            


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</section>